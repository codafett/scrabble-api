{
  "name": "license-api",
  "version": "2019.10.0",
  "license": "UNLICENSED",
  "private": true,
  "description": "AppScore Application Programming Interface",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:fp-license/license-api.git"
  },
  "main": "server.js",
  "scripts": {
    "compile": "babel --presets env,stage-0 -d lib/ app/",
    "prepublish": "npm run compile",
    "start": "babel-node app/server.js",
    "start:prod": "babel-node lib/server.js NODE_ENV=production LOG_LEVEL=info",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=DEBUG jest --coverage",
    "test:logging": "cross-env LOG_LEVEL=DEBUG jest --coverage",
    "test:no-coverage": "cross-env NODE_ENV=test LOG_LEVEL=DEBUG jest",
    "test:tdd": "cross-env NODE_ENV=test LOG_LEVEL=DEBUG jest --watch",
    "test:tdd-logging": "cross-env LOG_LEVEL=DEBUG jest --watch",
    "dev": "nodemon server.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . --ext .gql --ext .graphql",
    "lint:staged": "lint-staged",
    "coverage": "cross-env NODE_ENV=test nyc mocha",
    "docker:build": "docker-compose -f docker-compose.yml build",
    "docker:up": "docker-compose -f docker-compose.yml -f docker-compose-dev.yml up"
  },
  "author": "Reuben Greaves",
  "dependencies": {
    "@playlyfe/gql": "^2.6.0",
    "@shelf/jest-mongodb": "^1.1.0",
    "apollo-server-core": "^2.3.4",
    "apollo-server-express": "^2.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.1",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-istanbul": "5.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-http-context": "1.2.0",
    "express-jwt": "^5.3.1",
    "graphql": "^14.0.2",
    "jsonwebtoken": "^8.4.0",
    "jwt-simple": "^0.5.5",
    "lodash.merge": "^4.6.1",
    "merge-graphql-schemas": "^1.5.8",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.3.14",
    "mongoose-aggregate-paginate": "^1.1.3",
    "mongoose-lean-id": "^0.1.1",
    "mongoose-paginate": "^5.0.3",
    "mongoose-private-paths": "^0.7.0",
    "morgan": "^1.9.0",
    "nodejs-license-file": "^4.0.0",
    "nodemon": "1.18.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-token": "^1.0.1",
    "rand-token": "^0.4.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "easygraphql-tester": "^2.1.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-redux-saga": "^1.0.0",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "nyc": "^13.1.0",
    "prettier-eslint": "^8.8.1",
    "redux-saga": "^1.0.0",
    "shortid": "^2.2.14",
    "sinon": "^7.1.1",
    "sinon-mongoose": "^2.2.1",
    "sloc": "^0.2.0",
    "superagent": "^4.1.0",
    "supertest": "^3.3.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "class-methods-use-this": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "function-paren-newline": [
        "error",
        "consistent"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/no-webpack-loader-syntax": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "specialLink": [
            "Link"
          ]
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "object-curly-newline": [
        "error",
        {
          "consistent": true
        }
      ],
      "prefer-template": 2,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-default-props": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.babel.js"
        }
      }
    },
    "globals": {
      "BASE_API_URL": false
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "include": [
      "app/**/*.js"
    ],
    "exclude": [
      "app/**/*.test.js",
      "app/models/graphql/**/*.types.js",
      "app/config/database.js",
      "app/server.js",
      "app/apolloServer.js",
      "app/utils/logger.js",
      "app/utils/testUtils.js"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
